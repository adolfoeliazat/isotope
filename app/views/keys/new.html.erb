<div id="content">
  <div class="wrap" style="padding-left:10px;" >
    <div id="formbox">
<h1>New key</h1>

<% form_for(@key) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :title %><br />
    <%= f.text_field :title, :size => 53, :class => "title" %>
  </p>
  <p>
    <%= f.label :value %><br />
    <%= f.text_area :value, :rows => 10, :cols => 60, :class => "body" %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', keys_path %>
</div>
</div>
</div>
